{
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/(.*)$",

		"^@/api/(.*)$",
		"^@/app/(.*)$",
		"^@/assets/(.*)$",
		"^@/components/(.*)$",
		"^@/ui/(.*)$",
		"^@/layout/(.*)$",
		"^@/screens/(.*)$",
		"^@/templates/(.*)$",
		"^@/blocks/(.*)$",
		"^@/config/(.*)$",
		"^@/constants/(.*)$",
		"^@/fonts/(.*)$",
		"^@/hooks/(.*)$",
		"^@/providers/(.*)$",
		"^@/services/(.*)$",
		"^@/shared/(.*)$",
		"^@/store/(.*)$",
		"^@/utils/(.*)$",
		"^../(.*)",
		"^./(.*)",
		"(.scss)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
